{
  "name": "meal-suggestion",
  "version": "1.0.0",
  "description": "Gerador de refeições veganas com suas listas de ingredientes.",
  "main": "cypress.config.js",
  "scripts": {
    "lint": "eslint src/*.js && eslint cypress/e2e/*.js && eslint cypress.config.js",
    "lint:fix": "eslint src/*.js --fix && eslint cypress/e2e/*.js --fix && eslint cypress.config.js --fix",
    "cy:open": "cypress open",
    "cy:open:prod": "cypress open --env environment=prod",
    "test:local": "cypress run --browser chrome --env environment=local",
    "test:prod": "cypress run --browser chrome --env environment=prod",
    "test:local:cloud": "cypress run --browser chrome --env environment=local --record --tag 'local'",
    "test:prod:cloud": "cypress run --browser chrome --env environment=prod --record --tag 'prod'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/meal-suggestion.git"
  },
  "keywords": [],
  "author": "Walmyr <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wlsf82/meal-suggestion/issues"
  },
  "homepage": "https://github.com/wlsf82/meal-suggestion#readme",
  "devDependencies": {
    "cypress": "^15.8.1",
    "cypress-plugin-steps": "^1.2.1",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^3.6.0"
  }
}
